{
  "android": {
    "v8Flags": "--expose_gc"
  },
  "main": "app.js",
  "name": "tns-template-hello-world-ts",
  "version": "4.0.0",

  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
